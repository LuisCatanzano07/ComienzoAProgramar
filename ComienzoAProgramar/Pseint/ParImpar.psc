//Indica si un número entero dado es par o impar.(Se entiende elcero como par)
Algoritmo ParImpar
	Definir num Como Entero;
	num=0;
	Escribir 'Dime un número entero';
	Leer num;
	Si num%2=0 Entonces
		Escribir 'El número es par';
	SiNo
		Escribir 'El número es impar';
	FinSi	
FinAlgoritmo
